<?php

/**
 * @file
 * This module provides a taxonomy tag cloud.
 */

/**
 *  Implements hook_theme().
 */
function tagadelic_theme($existing, $type, $theme, $path) {
  return array(
    'tagadelic_taxonomy_cloud' => array(
      'variables' => array('tags' => NULL),
      'template' => 'tagadelic-taxonomy-cloud',
    )
  );
}
